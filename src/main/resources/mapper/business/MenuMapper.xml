<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.buzz.prototype.business.common.menu.MenuMapper">

	<select id="selectMenuById" parameterType="integer" resultType="com.buzz.prototype.business.base.ResultHashMap">
        SELECT 	SEQ
        		,MNU_ID
        		,MNU_NAME
        		,MNU_ORD 
        FROM	TBCO1001
        WHERE	MNU_ID = #{mnuId} 
    </select>
	
</mapper>